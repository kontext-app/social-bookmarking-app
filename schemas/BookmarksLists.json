{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "title": "BookmarksLists",
  "items": {
    "$ref": "#/definitions/BookmarksList"
  },
  "maxLength": 1024,
  "definitions": {
    "BookmarksList": {
      "properties": {
        "title": {
          "type": "string",
          "title": "title",
          "maxLength": 150
        },
        "description": {
          "type": "string",
          "title": "description",
          "maxLength": 1024
        },
        "creationDate": {
          "type": "string",
          "title": "bookmarkDate",
          "format": "date",
          "maxLength": 10
        },
        "tags": {
          "type": "array",
          "title": "tags",
          "items": {
            "type": "string"
          },
          "maxItems": 10
        },
        "upVotes": {
          "$ref": "#/definitions/DIDArray"
        },
        "downVotes": {
          "$ref": "#/definitions/DIDArray"
        },
        "bookmarks": {
          "type": "array",
          "title": "Bookmarks",
          "items": {
            "$ref": "#/definitions/Bookmark"
          },
          "maxLength": 1024
        }
      }
    },
    "Bookmark": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "title": "url",
          "maxLength": 240
        },
        "title": {
          "type": "string",
          "title": "title",
          "maxLength": 150
        },
        "description": {
          "type": "string",
          "title": "description",
          "maxLength": 1024
        },
        "highlight": {
          "type": "string",
          "title": "description",
          "maxLength": 1024
        },
        "creationDate": {
          "type": "string",
          "title": "bookmarkDate",
          "format": "date",
          "maxLength": 10
        },
        "tags": {
          "type": "array",
          "title": "tags",
          "maxItems": 10,
          "items": {
            "type": "string"
          }
        },
        "numberOfShares": {
          "type": "number",
          "title": "numberOfShares",
          "exclusiveMinimum": 0
        },
        "upVotes": {
          "$ref": "#/definitions/DIDArray"
        },
        "downVotes": {
          "$ref": "#/definitions/DIDArray"
        },
        "comments": {
          "$ref": "#/definitions/CeramicDocIdArray"
        }
      },
      "required": ["url", "title", "bookmarkDate"]
    },
    "DID": {
      "type": "string",
      "pattern": "^did:.+:.+",
      "maxLength": 1024
    },
    "DIDArray": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/DID"
      },
      "maxItems": 1024
    },
    "CeramicDocId": {
      "type": "string",
      "pattern": "^ceramic://.+(\\?version=.+)?",
      "maxLength": 1024
    },
    "CeramicDocIdArray": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/CeramicDocId"
      },
      "maxItems": 1024
    }
  }
}
