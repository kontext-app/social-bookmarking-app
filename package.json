{
  "name": "social-bookmarking-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "3id-connect": "1.0.0-alpha.6",
    "@ceramicnetwork/3id-did-resolver": "^0.5.3",
    "@ceramicnetwork/http-client": "^0.8.5",
    "@ceramicstudio/idx": "^0.4.2",
    "@ceramicstudio/idx-constants": "^0.4.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@walletconnect/web3-provider": "^1.3.1",
    "dids": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux-persist": "^6.0.0",
    "web3modal": "^1.9.1"
  },
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "start": "yarn build:style && react-scripts start",
    "start:ceramic": "ceramic daemon",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write './src/**/*.{js,md,json}'",
    "scripts:build": "tsc -p tsconfig.scripts.json",
    "idx:publish": "yarn scripts:build && yarn idx:publish:schemas && yarn idx:publish:definitions && yarn idx:constants",
    "idx:publish:schemas": "node ./scripts/build/scripts/publish-schemas.js",
    "idx:publish:definitions": "node ./scripts/build/scripts/publish-definitions.js",
    "idx:constants": "yarn scripts:build && node ./scripts/build/scripts/to-constants.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "3id-did-provider": "^0.1.1",
    "@ceramicstudio/idx-tools": "^0.4.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "autoprefixer": "^10.0.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "ethers": "^5.0.19",
    "postcss-cli": "^8.0.0",
    "prettier": "^2.1.2",
    "profile-hover": "^1.1.2",
    "tailwindcss": "^1.8.10",
    "typescript": "^4.0.5"
  }
}
